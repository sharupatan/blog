  <%= button_to "Log Out", destroy_user_session_path, data:{turbo:"false"}, method: :delete%>
<%else%>
  <%= link_to "Signin", new_user_session_path %>
  <%= link_to "Create Account", new_user_registration_path %>
<%end%>